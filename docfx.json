{
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": ["Assets/**/*.cs"],
          "exclude": ["Assets/Plugins/**"]
        }
      ],
      "globalNamespaceId": "Global",
      "filter": "filterConfig.yml",
      "dest": "api"
    }
  ],
  "build": {
    "template": ["default", "templates/unity"],
    "globalMetadata": {
      "_appTitle": "Kitten Rhythm Specification",
      "_appLogoPath": "resources/icon.svg",
      "_appFooter": "Kitten Rhythm Specification Â© 2022 zangwang",
      "_enableSearch": true
    },
    "content": [
      {
        "files": ["toc.yml", "README.md"]
      },
      {
        "src": "api",
        "files": ["*.yml"],
        "dest": "api"
      },
      {
        "src": "manual",
        "files": ["toc.yml", "*.md"],
        "dest": "manual"
      }
    ],
    "overwrite": [
      {
        "src": "..",
        "files": ["Assets/Scripts/**/*.md"],
        "exclude": ["obj/**", "_site/**"]
      }
    ],
    "resource": [
      {
        "files": ["resources/**/*"]
      }
    ],
    "xref": ["https://normanderwan.github.io/UnityXrefMaps/xrefmap.yml"],
    "xrefService": ["https://xref.docs.microsoft.com/query?uid={uid}"],
    "dest": "_site"
  }
}
